<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.astam.box_music2" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Box_music2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ress"
            android:label="@string/title_activity_ress" >
        </activity>
        <activity
            android:name=".Player"
            android:label="@string/title_activity_player"
            android:screenOrientation="portrait"
            android:parentActivityName=".Box_music2" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.astam.box_music2.Box_music2" />
        </activity>
    </application>

</manifest>
